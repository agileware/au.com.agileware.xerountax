<?xml version="1.0"?>
<extension key="au.com.agileware.xerountax" type="module">
  <file>xerountax</file>
  <name>Xero Un-tax</name>
  <description>Replaces exclusive tax amounts sent to Xero with inclusive amounts to avoid rounding issues</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>Agileware Support</author>
    <email>support@agileware.com.au</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/agileware/au.com.agileware.xerountax</url>
    <url desc="Documentation">https://github.com/agileware/au.com.agileware.xerountax</url>
    <url desc="Support">https://agileware.com.au/contact</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2021-10-25</releaseDate>
  <version>1.1</version>
  <develStage>stable</develStage>
  <compatibility>
    <ver>5.0</ver>
  </compatibility>
  <civix>
    <namespace>CRM/Xerountax</namespace>
    <format>25.01.1</format>
  </civix>
  <classloader>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
</extension>
